    "item": [
      {
        "name": "Register Camera",
        "request": {
          "method": "POST",
          "header": [],
          "body": {
            "mode": "raw",
            "raw": "{\n    \"camera_name\": \"Camera Name\",\n    \"region\": \"Camera Region Name\",\n    \"address\": \"Camera Addrress\",\n    \"max_speed_limit\": 75,\n    \"camera_url\": \"your_camera_url\",\n    \"camera_url_params\": \"your_camera_url_params\",\n    \"camera_username\": \"your_camera_username\",\n    \"camera_password\": \"your_camera_password\"\n}"
          },
          "url": {
            "raw": "http://localhost:5002/cameras/register",
            "protocol": "http",
            "host": ["localhost"],
            "port": "5002",
            "path": ["cameras", "register"]
          }
        },
        "response": []
      },
      {
        "name": "Update Camera",
        "request": {
          "method": "PUT",
          "header": [],
          "body": {
            "mode": "raw",
            "raw": "{\n    \"camera_name\": \"Camera name\",\n    \"region\": \"New Region\",\n    \"address\": \"New Address\",\n    \"max_speed_limit\": 80,\n    \"camera_url\": \"your_new_camera_url\",\n    \"camera_url_params\": \"your_new_camera_url_params\",\n    \"camera_username\": \"your_new_camera_username\",\n    \"camera_password\": \"your_new_camera_password\"\n}"
          },
          "url": {
            "raw": "http://localhost:5002/cameras/update",
            "protocol": "http",
            "host": ["localhost"],
            "port": "5002",
            "path": ["cameras", "update"]
          }
        },
        "response": []
      },
      {
        "name": "Delete Camera",
        "request": {
          "method": "DELETE",
          "header": [],
          "url": {
            "raw": "http://localhost:5002/cameras/delete/your_camera_id",
            "protocol": "http",
            "host": ["localhost"],
            "port": "5002",
            "path": ["cameras", "delete", "your_camera_id"]
          }
        },
        "response": []
      },
      {
        "name": "Get Frames",
        "request": {
            "method": "GET",
            "header": [],
            "url": {
                "raw": "http://localhost:5002/your_camera_id/frames/list?start_time=2023-11-10T00:00:00&end_time=2023-11-11T00:00:00&page=1&page_size=10&exceeded_max_speed=true",
                "protocol": "http",
                "host": ["localhost"],
                "port": "5002",
                "path": [
                  "cameras",
                  "your_camera_id", 
                  "frames", 
                  "list"
                ],
                "query": [
                    {
                        "key": "start_time",
                        "value": "2023-11-10T00:00:00",
                        "equals": true,
                        "description": ""
                    },
                    {
                        "key": "end_time",
                        "value": "2023-11-11T00:00:00",
                        "equals": true,
                        "description": ""
                    },
                    {
                        "key": "page",
                        "value": "1",
                        "equals": true,
                        "description": ""
                    },
                    {
                        "key": "page_size",
                        "value": "10",
                        "equals": true,
                        "description": ""
                    },
                    {
                        "key": "exceeded_max_speed",
                        "value": "true",
                        "equals": true,
                        "description": "Filter frames where vehicles exceeded maximum speed"
                    }
                ]
            }
        },
        "response": []
      },
      {
        "name": "Save Frame",
        "request": {
          "method": "POST",
          "header": [],
          "body": {
            "mode": "raw",
            "raw": "{\n    \"mac_address\": \"02:42:ac:11:00:02\",\n    \"processed_frame\": \"{...}\"  // Replace with your processed frame data\n}"
